{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "strict": true,
    "types": []
  },
  "include": ["src", "vite.config.ts"],
  "exclude": ["dist", "node_modules", "supabase/functions/server", "api", "types"]
}
